    "dotenv": "^10.0.0",
    "form-data": "^4.0.0",
    "graphql": "^16.1.0",
    "graphql-middleware": "^6.1.13",
    "graphql-redis-subscriptions": "^2.4.2",
    "graphql-relay": "^0.9.0",
    "graphql-scalars": "^1.14.1",
    "graphql-subscriptions": "^2.0.0",
    "graphql-type-json": "^0.3.2",
    "graphql-upload": "^13.0.0",
    "ioredis": "^4.28.2",
