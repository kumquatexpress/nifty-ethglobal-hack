{
  "name": "nifty-hacks-2022",
  "maintainer": "Boyang Niu",
  "license": "MIT",
  "version": "0.0.1",
  "main": "./build/app.js",
  "scripts": {
    "postinstall": "node ./scripts/setup_env.js",
    "start": "pm2 start",
    "start:server": "pm2 start --name server node ./build/app.js",
    "start:worker": "pm2 start --name worker node ./worker/build/worker/main.js",
    "build": "tsc --project ./ && tsc --project ./worker",
    "build:worker": "tsc --project ./worker",
    "restart": "pm2 restart all",
    "dev": "tsc-watch --onSuccess \"bash ./scripts/restart_dev.sh\"",
    "docker": "docker build -t nifty-ethglobal-hack_app:latest .",
    "migrate": "sequelize-cli db:migrate",
    "rollback": "sequelize-cli db:migrate:undo"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.36.0",
