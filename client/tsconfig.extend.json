{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@modules/*": ["styles/modules/*"],
      "@lib/*": ["scripts/lib/*"],
      "@utils/*": ["utils/*"],
      "@scripts/*": ["scripts/*"],
      "@styles/*": ["styles/*"],
      "@graphql/*": ["graphql/*"],
      "@gqlt/*": ["graphql/__generated__/*"]
    }
  }
}
